			<Word>Multi</Word>

			<!-- Proper english spelling! -->
			<Word>Humanise</Word>

			<!-- A valid shortened version of Utilities -->
			<Word>Utils</Word>
			
			<!-- Short version of Pixel -->
			<Word>px</Word>
			
			<!-- Json is valid -->
			<Word>Json</Word>
		</Recognized>
		<Unrecognized>
			<!-- Words that should cause a violation. -->
		</Unrecognized>
	</Words>
	<Acronyms>
