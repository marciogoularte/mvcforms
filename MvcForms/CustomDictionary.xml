			<!-- Words that should not cause a violation. -->
			
			<!-- A valid shortening of Model View Controller -->
			<Word>Mvc</Word>
			
			<!-- An object used to render an element. -->
			<Word>Renderer</Word>

			<!-- A HTML common Element. -->
